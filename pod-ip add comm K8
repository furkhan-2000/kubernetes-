apiVersion: networking.k8s.io/v1
kind: NetworkPolicy
metadata:
  name: ola
  namespace: uber
spec:
  podSelector:
    matchLabels:
      role: bottle
  policyTypes:
  - Ingress
  - Egress
  ingress:
  - from:
    - ipBlock:
        cidr: 172.31.32.0/20
  egress:
  - to:
    - ipBlock:
        cidr: 172.0.0.0/16
