apiVersion: networking.k8s.io/v1
kind: NetworkPolicy
metadata:
  name: sovet
  namespace: tomorrow
spec:
  podSelector:
    matchLabels:
      role: bag
  policyTypes:
  - Ingress
  - Egress
  ingress:
  - from:
    - podSelector:
        matchLabels:
          role: bug
  egress:
  - to:
    - podSelector:
        matchLabels:
          role: bug
    - ipBlock:
        cidr: 172.31.32.0/20
