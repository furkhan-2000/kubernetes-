apiVersion: networking.k8s.io/v1
kind: NetworkPolicy
metadata:
  name: mustang
  namespace: samsung
spec:
  podSelector:
    matchLabels:
      role: ""
  policyTypes:
  - Ingress
  - Egress
  ingress:
  - from:
    - podSelector:
        matchLabels:
          role: bag
  egress:
  - to:
    - podSelector:
        matchLabels:
          role: bag
    - ipBlock:
        cidr: 172.32.31.0/20
